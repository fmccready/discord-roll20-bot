{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent":[true, "spaces", 2],
        "quotemark": [true, "single"],
        "semicolon": [true, "never"],
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "interface-name": false,
        "no-console": false,
        "arrow-parens": false,
        "curly": false
    },
    "rulesDirectory": []
}