{
  "name": "discord-roll20-bot",
  "version": "0.0.1",
  "description": "A bot that will let people signup for a roll20 game on discord.",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "watch-webpack": "NODE_ENV='development' webpack --watch --config webpack.config.js",
    "webpack": "webpack --config webpack.config.js",
    "start": "tsc && npm run webpack && node dist/server.js",
    "dev": "concurrently \"nodemon\" \"npm run watch-webpack\"",
    "test": "jest",
    "refresh-test-data": "node -r ts-node/register test/refresh-test-data.ts",
    "heroku-postbuild": "webpack -p --config webpack.config.js"
  },
  "keywords": [
    "roll20",
    "discord",
    "chatbot"
  ],
  "author": "Frank McCready",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "discord.js": "^12.2.0",
    "express": "^4.17.1",
    "pg": "^7.4.3",
    "pug": "^3.0.0",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rxjs": "^6.2.1",
    "sequelize": "^5.19.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.21",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/pug": "^2.0.4",
    "@types/ramda": "^0.27.6",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/rx-core": "^4.0.3",
    "@types/sinon": "^7.0.3",
    "@types/validator": "^13.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "concurrently": "^3.6.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^2.1.1",
    "dotenv": "^6.2.0",
    "jest": "^26.0.1",
    "json-cycle": "^1.3.0",
    "nodemon": "^2.0.4",
    "prettier": "1.13.7",
    "pug-lint": "^2.6.0",
    "sinon": "^7.2.2",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^5.11.0",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "14.3.0"
  }
}
