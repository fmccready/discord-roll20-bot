{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "jsxRules": {
        "quotemark": [
            true,
            "double"
        ]
    },
    "rules": {
        "trailing-comma": true,
        "indent": [
            true,
            "spaces",
            2
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "object-literal-shorthand": false,
        "only-arrow-functions": false,
        "interface-name": false,
        "no-console": false,
        "arrow-parens": false,
        "curly": false
    },
    "rulesDirectory": []
}