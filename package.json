{
  "name": "discord-roll20-bot",
  "version": "0.0.1",
  "description": "A bot that will let people signup for a roll20 game on discord.",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && tsc",
    "build-webpack": "webpack --config webpack.config.js",
    "build-production-webpack": "webpack -p --config webpack.config.js",
    "watch-webpack": "webpack --watch --config webpack.config.js",
    "start": "npm run build && npm run build-production-webpack && node dist/app.js",
    "dev": "npm run build && concurrently \"node dist/app.js\" \"npm run watch-webpack\"",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "heroku-postbuild": "npm run build-production-webpack"
  },
  "keywords": [
    "roll20",
    "discord",
    "chatbot"
  ],
  "author": "Frank McCready",
  "license": "MIT",
  "dependencies": {
    "btoa": "^1.2.1",
    "bufferutil": "^3.0.5",
    "cookie-parser": "^1.4.3",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pg-promise": "^8.4.5",
    "ramda": "^0.25.0",
    "react": "^16.4.1",
    "rxjs": "^6.2.1",
    "sequelize": "^4.38.0",
    "socket.io": "^2.1.1",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.21",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.4",
    "@types/node": "^9.6.23",
    "@types/ramda": "^0.25.34",
    "@types/rx-core": "^4.0.3",
    "@types/sequelize": "^4.27.25",
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.5.3",
    "less-loader": "^4.1.0",
    "mocha": "^5.2.0",
    "prettier": "1.13.7",
    "rewire": "^4.0.1",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^2.1.5"
  },
  "engines": {
    "node": "10.13.0"
  }
}
